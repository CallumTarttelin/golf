{
  "name": "golf",
  "version": "1.0.0",
  "description": "Golf Score Thing",
  "main": "index.js",
  "scripts": {
    "start": "webpack",
    "deploy": "webpack --config webpack.production.config.js",
    "lint": "./node_modules/.bin/eslint src test",
    "posttest": "npm run lint",
    "test": "mocha --require ignore-styles --compilers js:babel-register --colors --reporter mocha-multi --reporter-options spec=-,xunit=target/reports/js/unit/test-results.xml src/test/js/specs/helpers/browser.js 'src/test/js/specs/**/*.spec.js'",
    "test:watch": "mocha -w --require ignore-styles --compilers js:babel-register --colors --reporter spec src/test/js/specs/helpers/browser.js 'src/test/js/specs/**/*.spec.js'",
    "postcover": "npm run lint",
    "cover": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover --root 'src/main/js' --dir 'build/js-coverage' --excludes 'src/main/js/main.js' _mocha -- --require ignore-styles ./src/test/js/specs/helpers/browser.js './src/test/js/specs/**/*.spec.js'",
    "e2e-setup": "selenium-standalone install  --version=2.53.1 --drivers.chrome.version=2.24",
    "selenium": "selenium-standalone start",
    "e2e": "./node_modules/.bin/nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saskcow/golf/tree/master/app"
  },
  "author": "Callum Tarttelin",
  "license": "FreeBSD",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-root-slash-import": "^1.1.0",
    "browser-sync": "^2.13.0",
    "browser-sync-webpack-plugin": "^1.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^5.2.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.24.1",
    "http-proxy-middleware": "^0.17.2",
    "ignore-styles": "^5.0.1",
    "isparta": "^4.0.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.12.1",
    "mocha-multi": "^0.9.1",
    "nightwatch": "^0.9.8",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.0.2",
    "redux-mock-store": "^1.0.2",
    "sass-loader": "^3.2.0",
    "selenium-standalone": "^5.7.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webfontloader": "^1.6.26",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "rc-tree-select": "^1.8.6",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-bootstrap": "^0.23.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "history": "^2.1.2"
  }
}
